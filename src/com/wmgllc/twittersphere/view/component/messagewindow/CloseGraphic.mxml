<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" width="20" height="20">
	
	<fx:Script>
		<![CDATA[
			[Bindable]
			public var fillColour : uint;
		]]>
	</fx:Script>
	
	<s:Ellipse width="20" height="20">
		<s:fill>
			<s:SolidColor id="colourFill" color="{fillColour}"/>
		</s:fill>
	</s:Ellipse>
	<s:Line xFrom="6" xTo="14" yFrom="6" yTo="14">
		<s:stroke>
			<s:SolidColorStroke color="0xFFFFFF" weight="4"/> 
		</s:stroke>
	</s:Line>
	<s:Line xFrom="14" xTo="6" yFrom="6" yTo="14">
		<s:stroke>
			<s:SolidColorStroke color="0xFFFFFF" weight="4"/> 
		</s:stroke>
	</s:Line>
</s:Group>
